#include <iostream>
#include <math.h>

using namespace std;

int main(){
	int n,m, s=0,x;
	cout<<"Введите n:"<<endl;
	cin>>n;
	cout<<"Введите m:"<<endl;
	cin>>m;
	int a[n][m];
	cout<<"Введите элементы массива:"<<endl;
	for (int i=0;i<n;i++)
        for (int j=0;j<m;j++)
    {
        cin>>x;
        a[i][j]=x;
        if (x>0) s=s+x;

    }
    cout<<"sum="<<s<< endl;

	system("pause");
}
