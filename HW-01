#include <iostream>
#include <math.h>

using namespace std;

int main() {
    double a,b,c,x;
    cin >> a >> b >> c;
        x=(c-b)/a;
        cout << x << endl;

    return 0;
}

int main() {
int val;
cin>>val;
if(val == 0)
    cout<<"Число равно 0\n";
else if(val < 0)
    cout<<"Число отрицательное\n";
else
    cout<<"Число положительное\n";
 
    return 0;
}
